{
  "rebootDevice" :
  {
    "CCU": ["reboot.sh 0"]
  },
  "deleteAllJobs":
  { 
    "CCU":
    [
      "rm -rf /data/encrypted/cdp_app/*/*",
      "rm -rf /data/encrypted/rda_app/*/*",
      "rm -rf /data/encrypted/temptpfs/cdp_app/*/*",
      "rm -rf /data/encrypted/temptpfs/rda_app/*/*",
      "rm -rf /data/encrypted/obu_app/FOTA/DATA/TASKS/*",
      "rm -rf /data/unsigned/mbs/FOTA/*/*",
      "rm -rf /data/encrypted/rda_app/pending-requests.pb_txt",
      "rm -rf /data/encrypted/sua_app/task_control_installation_request.txt"
    ]
  },
  "getKeystorePath":{
    "CCU":["cat /media/bosch/mbs/mprm.prs | grep -w 'javax.net.ssl.keyStore' | cut -d '=' -f  2"]
  },
  "accessToModifyDeviceFiles":{
    "CCU":""
  },

  "stopGSMConnection":{
    "CCU":["systemctl stop gsm_component.service"]
  },
  
  "startGSMConnection":{
    "CCU":["systemctl start gsm_component.service"]
  },

  "stopGSM_MBS_Connection":{
    "CCU":["systemctl stop ota_client"]
  },

  "startGSM_MBS_Connection":{
    "CCU":["systemctl start ota_client"]
  },

  "getPairingDeviceStatus":{
    "CCU":"tdr_get_config_value integer BACKEND_VEHICLE_PAIRING_STATUS"
  },

  "readCCUVersionLineCTPGVersionJSONFile":{
    "CCU":"cat /etc/topas/ctpg_version.json | grep 'ccu6430_main'"
  },

  "readMBSLineCTPGVersionJSONFile":{
    "CCU":"cat /etc/topas/ctpg_version.json | grep 'mbs'"
  },

  "readDeviceCustomer":{
    "CCU":"cfgclient -r -t CS -p 0x03F0"
  },
  
  "deleteDeviceLogs":{
    "CCU":
    [
      "rm -rf /data/unsigned/mbs/logs/osgi/*.gz",
      "rm -rf /var/log/messages*"
    ]
  },

  "getSelfUpdateStatus":{
    "CCU":"awk '/remove new version folder/,/seconds/' /var/log/messages"
  },

  "getFullUpdateStatus":{
    "CCU":"cat /home/topas/software_update/state_files/update_result"
  },

  "peaSelfUpdateDatabase":{
    "CCU" : "cat /data/encrypted/pea_app/STORE/database.xml"
  },

  "peaSelfUpdateConfig":{
    "CCU" : "cat /data/encrypted/pea_app/CONFIGS/config.xml"
  },

  "zipCrtFolder":{
    "CCU" : "zip -r /data/encrypted/crt.zip /data/encrypted/crt"
  },

  "getCrtFolder":{
    "CCU" : "/data/encrypted/crt.zip"
  },

  "implantDeviceProfileIdPACCAR":{
    "CCU" : "cfgclient -w -t CS -p 0x03F0 -v 0x50 0x01"
  },

  "implantDeviceProfileIdHINO":{
    "CCU" : "cfgclient -w -t CS -p 0x03F0 -v 0x48 0x01"
  },

  "implantDeviceProfileIdPACCAR_NON_NGD":{
    "CCU" : "cfgclient -w -t CS -p 0x03F0 -v 0x50 0x02"
  },

  "implantEsim":{
    "CCU" : "cfgclient -w -t CS -p 0x1210 -v 01"
  },

  "implantLTEAntenna":{
    "CCU" : "cfgclient -w -t CS -p 0x1267 -v 02"
  },

  "implantDeviceSerialNumber":{
    "CCU" : "cfgclient -w -t CS -p 0x3E87 -v "
  },

  "getmprmFile":{
    "CCU" : "cat /data/unsigned/mbs/mprm.prs"
  },

  "getCCUVersion":{
    "CCU":"cat /etc/topas/ctpg_version.json | grep 'Short_Version'"
  },
  "setDate":{
    "CCU":"date -s '{device_time}'"
  },
  "implantDeviceVIN":{
    "CCU" : "cfgclient -w -t CS -p 0x1304 -v "
  }
}